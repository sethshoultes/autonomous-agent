{
  "dashboard": {
    "id": null,
    "title": "Autonomous Agent - Production Overview",
    "tags": ["autonomous-agent", "production", "overview"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "row",
        "collapsed": false,
        "datasource": null,
        "panels": []
      },
      {
        "id": 2,
        "title": "Application Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"autonomous-agent-app\"}",
            "legendFormat": "App Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "displayMode": "basic",
              "orientation": "horizontal"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "text": "Down"
                  },
                  "1": {
                    "color": "green",
                    "text": "Up"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 1
        }
      },
      {
        "id": 3,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"autonomous-agent-app\"}[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true
          }
        ],
        "xAxes": [
          {
            "show": true,
            "mode": "time"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 1
        }
      },
      {
        "id": 4,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"autonomous-agent-app\"}[5m]))",
            "legendFormat": "95th Percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"autonomous-agent-app\"}[5m]))",
            "legendFormat": "50th Percentile",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 9
        }
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"autonomous-agent-app\", status=~\"5..\"}[5m])",
            "legendFormat": "5xx Errors/sec",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{job=\"autonomous-agent-app\", status=~\"4..\"}[5m])",
            "legendFormat": "4xx Errors/sec",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 9
